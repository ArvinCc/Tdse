<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="orbbecVipMapper">

    <select id="findById" parameterType="int" resultType="com.tdse.mx.dao.OrbVip">
        select * from tdse_orbbec_vip where
        vip_id =#{vip_id}
    </select>

    <select id="findByType" parameterType="String" resultType="com.tdse.mx.dao.OrbVip">
        select * from tdse_orbbec_vip where
        vip_type =#{vip_type}
    </select>

    <select id="find" resultType="com.tdse.mx.dao.OrbVip">
        select * from tdse_orbbec_vip
    </select>


    <insert id="addOrbbecVip" parameterType="com.tdse.mx.dao.OrbVip">
        insert into tdse_orbbec_vip(vip_id,vip_type,vip_time,vip_price) values (#{vip_id},#{vip_type},#{vip_time},#{vip_price})
    </insert>

    <delete id="deleteById" parameterType="int">
        delete from tdse_orbbec_vip where
        vip_id=#{vip_id}
    </delete>

    <update id="updateOrbbecVip" parameterType="com.tdse.mx.dao.OrbVip">
        update tdse_orbbec_vip set vip_type=#{vip_type},vip_time=#{vip_time},vip_price=#{vip_price}
        where vip_id=#{vip_id}
    </update>

</mapper>